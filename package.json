{
  "name": "@staxe/staxe-contracts",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx hardhat compile",
    "test": "npx hardhat test",
    "format": "npx prettier --write .",
    "contracts:clean": "npx hardhat clean",
    "contracts:compile": "npx hardhat compile",
    "contracts:develop": "npx hardhat node --watch --tags develop",
    "contracts:test": "npx hardhat test",
    "contracts:coverage": "npx hardhat coverage",
    "deploy:rinkeby": "npx hardhat deploy --network rinkeby --tags test",
    "deploy:kovan": "npx hardhat deploy --network kovan --tags test",
    "deploy:mumbai": "npx hardhat deploy --network mumbai --tags test",
    "deploy:matic": "npx hardhat deploy --network matic --tags test"
  },
  "private": true,
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.5",
    "@nomiclabs/hardhat-etherscan": "3.0.1",
    "@nomiclabs/hardhat-waffle": "2.0.2",
    "@typechain/ethers-v5": "9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/mocha": "9.1.0",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "chai": "4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "5.5.4",
    "hardhat": "2.8.4",
    "hardhat-contract-sizer": "2.4.0",
    "hardhat-deploy": "0.10.5",
    "hardhat-deploy-ethers": "0.3.0-beta.13",
    "hardhat-gas-reporter": "1.0.8",
    "hardhat-typechain": "0.3.5",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.27",
    "prettier": "2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-node": "~9.1.1",
    "typechain": "7.0.0",
    "typescript": "4.5.5"
  }
}
